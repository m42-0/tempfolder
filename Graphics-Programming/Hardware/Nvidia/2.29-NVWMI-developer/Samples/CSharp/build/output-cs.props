<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Platform)' == 'AnyCPU'">
    <OutputPath>$(SolutionDir)_out\AnyCPU-$(Configuration)\</OutputPath>
    <OutDir>$(SolutionDir)_out\AnyCPU-$(Configuration)\</OutDir>
    <IntermediateOutputPath>$(SolutionDir)obj\$(AssemblyName)\AnyCPU-$(Configuration)\</IntermediateOutputPath>
    <IntDir>$(SolutionDir)_obj\$(AssemblyName)\AnyCPU-$(Configuration)\</IntDir>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Platform)' == 'x86'">
    <OutputPath>$(SolutionDir)_out\Win32-$(Configuration)\</OutputPath>
    <OutDir>$(SolutionDir)_out\Win32-$(Configuration)\</OutDir>
    <IntermediateOutputPath>$(SolutionDir)obj\$(AssemblyName)\Win32-$(Configuration)\</IntermediateOutputPath>
    <IntDir>$(SolutionDir)_obj\$(AssemblyName)\Win32-$(Configuration)\</IntDir>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Platform)' == 'x64'">
    <OutputPath>$(SolutionDir)_out\x64-$(Configuration)\</OutputPath>
    <OutDir>$(SolutionDir)_out\x64-$(Configuration)\</OutDir>
    <IntermediateOutputPath>$(SolutionDir)obj\$(AssemblyName)\x64-$(Configuration)\</IntermediateOutputPath>
    <IntDir>$(SolutionDir)_obj\$(AssemblyName)\x64-$(Configuration)\</IntDir>
  </PropertyGroup>
</Project>
