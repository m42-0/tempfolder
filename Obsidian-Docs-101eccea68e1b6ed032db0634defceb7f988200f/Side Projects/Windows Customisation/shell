shell

[HKEY_CLASSES_ROOT\*\shell\runas]
@="Take Ownership"
"NoWorkingDirectory"=""

[HKEY_CLASSES_ROOT\*\shell\runas\command]
@="cmd.exe /c takeown /f \"%1\" && icacls \"%1\" /grant administrators:F"
"IsolatedCommand"="cmd.exe /c takeown /f \"%1\" && icacls \"%1\" /grant administrators:F"

[HKEY_CLASSES_ROOT\Directory\shell\runas]
@="Take Ownership"
"NoWorkingDirectory"=""

[HKEY_CLASSES_ROOT\Directory\shell\runas\command]
@="cmd.exe /c takeown /f \"%1\" /r /d y && icacls \"%1\" /grant administrators:F /t"
"IsolatedCommand"="cmd.exe /c takeown /f \"%1\" /r /d y && icacls \"%1\" /grant administrators:F /t"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans]
"icon"="powercpl.dll"
"muiverb"="Power Plans"
"position"="top"
"subcommands"=""

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\01highperformance]
"icon"="powercpl.dll"
"muiverb"="Performance Plan (Idle Disabled)"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\01highperformance\command]
@="powercfg.exe /setactive 8c5e7fda-e8bf-4a96-9a85-a6e23a8c635c"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\02balanced]
"icon"="powercpl.dll"
"muiverb"="Balanced Plan (Idle Enabled)"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\02balanced\command]
@="powercfg.exe /setactive 381b4222-f694-41f0-9685-ff5bb260df2e"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\03openpoweroptions]
"icon"="powercpl.dll"
"muiverb"="Power Options"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\01powerplans\shell\03openpoweroptions\command]
@="control.exe powercfg.cpl"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\02gameboost]
"icon"="joy.cpl"
"muiverb"="Game Boost"
"position"="top"
"subcommands"=""

[HKEY_CLASSES_ROOT\DesktopBackground\shell\02gameboost\shell\01boost]
"icon"="joy.cpl"
@="On"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\02gameboost\shell\01boost\command]
@="powerrun boost.bat"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\02gameboost\shell\02unboost]
"icon"="joy.cpl"
@="Off"

[HKEY_CLASSES_ROOT\DesktopBackground\shell\02gameboost\shell\02unboost\command]
@="powerrun unboost.bat"




